{
   "manifest_version": 3,
   "name": "YouTube Lead Collector",
   "description": "Collect YouTube lead with channel name, channel url, subscribers count and email address and save to google spreadsheet",
   "author": "developer@misbahurbd.com",
   "version": "0.0.1",
   "background": {
      "service_worker": "backgorund.js"
   },
   "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
   },
   "action": {
      "default_title": "YT Lead Collector",
      "default_icon": {
         "16": "images/icon-16.png",
         "32": "images/icon-32.png",
         "48": "images/icon-48.png",
         "128": "images/icon-128.png"
      }
   },
   "permissions": ["storage", "declarativeContent"]
}
